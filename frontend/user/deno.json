{
  "tasks": {
    "dev": "deno run -A npm:@remix-run/dev vite:dev",
    "build": "deno run -A npm:@remix-run/dev vite:build",
    "typecheck": "deno check '**/*'",
    "prisma:pull": "deno run -A npm:prisma db pull",
    "prisma:generate": "deno run -A npm:prisma generate --sql --watch"
  },
  "compilerOptions": {
    "lib": ["deno.window", "dom"],
    "jsx": "react-jsx"
  }
}